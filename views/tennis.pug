extends layout

block content
  h1= title
  - let tennises=result
    
    div(class="container-fluid mt-3")
    div(class="row")
        each tennis in tennises
            div(class="col-sm-2 bg-primary m-1 p-3" onclick=`buy(${tennis})`) #{tennis.Player_Name} - #{tennis.Player_Age} - #{tennis.No_Of_Matches_Played}

    
    script.
     
      function buy(itemNo)
      {
        alert(`You purchased Item ${itemNo+1}`)
      }